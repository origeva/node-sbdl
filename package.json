{
	"name": "sbdl",
	"version": "0.0.1",
	"description": "YouTube downloader trimming skip segments fetched from SponsorBlock.",
	"main": "./lib/index.js",
	"types": "./lib/index.d.ts",
	"files": [
		"lib"
	],
	"homepage": "https://github.com/origeva/node-sbdl",
	"repository": {
		"type": "git",
		"url": "https://github.com/origeva/node-sbdl"
	},
	"scripts": {
		"dev": "nodemon",
		"test": "npm run build && (echo Testing... & mocha -t 3000 -r ts-node/register --recursive \"./test/**/*.ts\")",
		"build": "npm run clean && (echo Building... & tsc)",
		"clean": "echo Cleaning... & rd /s /q \"./lib\" & exit",
		"prepublishOnly": "npm test"
	},
	"keywords": [
		"youtube",
		"sponsorblock",
		"ffmpeg",
		"video",
		"audio",
		"download"
	],
	"author": "Ori Geva",
	"license": "MIT",
	"dependencies": {
		"ffmpeg-static": "^4.4.0",
		"sponsorblock-api": "^0.2.2",
		"ytdl-core": "^4.9.1"
	},
	"devDependencies": {
		"@types/chai": "^4.2.14",
		"@types/ffmpeg-static": "^3.0.0",
		"@types/mocha": "^8.2.0",
		"@types/node": "^14.17.12",
		"chai": "^4.2.0",
		"mocha": "^8.2.1",
		"nodemon": "^2.0.6",
		"ts-node": "^9.1.1",
		"typescript": "^4.1.3"
	}
}
